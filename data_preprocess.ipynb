    "def readlist():\n",
    "    '''Read the label as a dict from labelfile'''\n",
    "    mylist = []\n",
    "    with open(patientlist, 'r') as f:\n",
    "        flines = f.readlines()\n",
    "        for line in flines:\n",
    "            data= line.split()\n",
    "            mylist.append(data[0])\n",
    "    return mylist\n"
